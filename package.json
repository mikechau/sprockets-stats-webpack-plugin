  "repository": {
    "type": "git",
    "url": "git+https://github.com/mikechau/sprockets-stats-webpack-plugin.git"
  },
  "keywords": [
    "sprockets",
    "rails",
    "assets",
    "manifest",
    "webpack",
    "plugin"
  ],
  "author": "Mike Chau (@mikechau)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/mikechau/sprockets-stats-webpack-plugin/issues"
  },
  "homepage": "https://github.com/mikechau/sprockets-stats-webpack-plugin#readme",
  "devDependencies": {
    "ava": "^0.15.2",
    "css-loader": "^0.23.1",
    "eslint": "^2.13.1",
    "eslint-config-mc": "^0.6.5",
    "eslint-plugin-ava": "^2.5.0",
    "eslint-plugin-import": "^1.8.1",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.9.0",
    "memory-fs": "^0.3.0",
    "remove-webpack-plugin": "^1.1.1",
    "sri-stats-webpack-plugin": "^0.7.3",
    "style-loader": "^0.13.1",
    "url-loader": "^0.5.7",
    "webpack": "^1.13.1"
  },
  "dependencies": {
    "loader-utils": "^0.2.15",
    "lodash.merge": "^4.4.0",
    "walk": "^2.3.9",
    "webpack-custom-stats-patch": "^0.3.0"
  },
  "ava": {
