{
  "name": "sprockets-stats-webpack-plugin",
  "version": "0.3.0",
  "description": "Add sprockets metadata to webpack stats for asset manifest generation.",
  "main": "index.js",
  "files": [
    "index.js",
    "lib"
  ],
  "scripts": {
    "test": "ava",
    "lint": "eslint ./test ./lib index.js",
